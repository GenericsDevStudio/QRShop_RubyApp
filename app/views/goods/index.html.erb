<h1 class="text text-center">Goods</h1><br />
  <%= link_to '<- Users', users_path %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Code</th>
      <th scope="col">Count</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @goods.each do |obj| %>
      <tr>
        <th scope="row"><%= obj.idG %></th>
        <td><%= obj.name %></td>
        <td><%= obj.price %></td>
        <td><%= obj.code %></td>
        <td><%= obj.count %></td>
        <td><%= link_to 'edit', goods_edit_path(idG: obj.idG, name: obj.name, price: obj.price, count: obj.count) %> |
           <%= link_to 'delete', goods_remove_path(idG: obj.idG), class: "text-danger", data: { confirm: "You sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br />
<div class="text text-center">
  <%= link_to 'New Good', new_good_path, class: "btn btn-primary" %>
</div>
